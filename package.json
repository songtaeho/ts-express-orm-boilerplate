{
  "name": "@taeho/ts-express-orm-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "TS_NODE_FILES=true node --inspect=5858 -r ts-node/register ./src/server.ts",
